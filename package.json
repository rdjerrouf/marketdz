{
  "name": "marketdz",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack & start http://localhost:3000",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "test": "npx playwright test",
    "test:headed": "npx playwright test --headed",
    "test:ui": "npx playwright test --ui",
    "test:report": "npx playwright show-report",
    "test:pool": "node scripts/test-connection-pool.js",
    "mock:test": "node scripts/quick-mock-data.js test",
    "mock:medium": "node scripts/quick-mock-data.js medium", 
    "mock:full": "node scripts/quick-mock-data.js full",
    "health": "curl http://localhost:3000/api/health",
    "docker:build": "docker-compose build app",
    "docker:up": "docker-compose --env-file .env.docker up -d app",
    "docker:down": "docker-compose down",
    "docker:logs": "docker logs marketdz-app-1 --follow",
    "docker:restart": "docker-compose restart app",
    "docker:reset": "docker-compose down && docker-compose up -d app",
    "docker:shell": "docker exec -it marketdz-app-1 sh",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:status": "docker ps && echo '\n--- Networks ---' && docker network ls"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.56.1",
    "@upstash/redis": "^1.35.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "next-pwa": "^5.6.0",
    "playwright": "^1.55.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "workbox-webpack-plugin": "^7.3.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^10.0.0",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
